@article{ WOS:000215253800001,
Author = {Tu Chuan-Fei and Green, Thomas A. and Zheng Guo-Hua and Qiang, Feng},
Title = {Dragon Dance in Tu Village: Social Cohesion and Symbolic Warfare},
