@article{ WOS:000222295300009,
Author = {Mazar, A},
Title = {Ritual dancing in the iron age},
Journal = {NEAR EASTERN ARCHAEOLOGY},
Year = {2003},
Volume = {66},
Number = {3},
Pages = {126+},
Month = {SEP},
Abstract = {In Old Testament times, dancing was an important, ritualized activity.
   It is mentioned in several occasions in the Hebrew Bible in connection
   with ritual or victory ceremonies, as part of mourning rites (Eccl 3:4),
   or in literary metaphors where animals like rams, goats and sheep are
   described as dancing (e.g., Psalms 29:6; 114:4; King and Stager 2001:
   298-300). Mainly dances were performed by women. They are described as
   dancing with drums following major military victories, for example,
   Miriam (Exodus 15:20), Jephthah's daughter (Judg 11:34) and the women
   approaching David after he defeated Goliath (1 Sam 18:6; compare also I
   Sam 21:12; 29:5; Jer 31:4). Elsewhere, dancing is related to ritual, as
   in the case of the golden calf ritual (Exod 32:19), the celebration at
   Shiloh (Judg 21:21) or the bringing of the arc to Jerusalem (2 Sam
   6:14-16).},
