@article{ WOS:000723886800002,
Author = {Arcangeli, Alessandro},
Title = {The Dance of Discoveries and the Discovery of Dances},
Journal = {ITINERARIA},
Year = {2021},
Volume = {20},
Pages = {9-22},
Abstract = {As for all categories, an event in which travellers and discoverers
   participate or attend can be qualified as a dance fundamentally on the
   basis of a culturally situated perspective, in the eye of the beholder.
   Whatever terms and concepts are conjured in one language or another, the
   definitions that come into play associate a particular social practice
   with a series of others, to whose sphere (whether artistic, religious or
   political) it is correlated; and with consistent evaluations (aesthetic,
   moral and functional). While starting from its own tradition, the
   (Western) discourse on dance adapts and develops in the renaissance,
   drawing inspiration from the experience of cultural encounters that so
   strongly characterize the era. in addition to enriching the heritage of
   known and inventoried practices, the opportunities for cultural
   exchanges and hybridizations are amplified.},
