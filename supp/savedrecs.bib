@article{ WOS:000208020400005,
Author = {Eddy, Mike},
Title = {Pre-Hispanic Music Survivals from the Canary Islands},
Journal = {AL-MASAQ-ISLAM AND THE MEDIEVAL MEDITERRANEAN},
Year = {2009},
Volume = {21},
Number = {2},
Pages = {163-173},
Abstract = {This article discusses the evidence, mainly ethno-historical, for music
   and dance in the Canary Islands prior to the incorporation of the
   archipelago in the Crown of Castile during the fifteenth century. The
   survival of essential elements of pre-Hispanic music and dance is noted
   in some present-day dance forms, as are related customs in some of the
   islands' main festivals. The relationship between pre-Hispanic dance and
   the Renaissance and early modern European dance, in the Canaries, is
   described and identified as an area for future research.},
