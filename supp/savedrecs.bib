@article{ WOS:000480051400001,
Author = {Gurr, Mel},
Title = {Celebratory Socialism: Subcultural Politics of Dancing, Drinking, and
   Hooking Up Among Youth of Brazil's MST},
Journal = {JOURNAL OF LATIN AMERICAN AND CARIBBEAN ANTHROPOLOGY},
Year = {2019},
Volume = {24},
Number = {3},
Pages = {690-708},
Month = {NOV},
Abstract = {Based on participant observation at the Brazil's Landless Rural Workers'
   Movement's (MST's) National Congress in 2014, this article explores
   embodied dimensions of corporeality as a major site of learning and
   symbolic elaboration among rural youth affiliated with MST. By examining
   rituals of drinking, dancing, and hooking up in the aftermath of police
   repression, it demonstrates the ways in which landless youth craft their
   own socialist subculture where they obtain an experiential glimpse of
   what a more egalitarian society might feel like. At the same time, these
   domains shed light on broader negotiations of gender, sexuality, and
   belonging within Brazilian society and in the MST. By paying attention
   to the political implications of pleasurable practices, it argues that
   celebrating socialism is a source of resilience and strength, fostering
   youth involvement in emancipatory political projects in troubled times.
