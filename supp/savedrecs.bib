@article{ WOS:000399597800004,
Author = {Cremona, Vicki Ann},
Title = {Drawing back the curtains on the actor's `private place': a personal
   journey into ISTA 2016},
Journal = {THEATRE DANCE AND PERFORMANCE TRAINING},
Year = {2017},
Volume = {8},
Number = {1},
Pages = {33-45},
Month = {MAR},
Abstract = {This article describes a personal journey into the International School
   of Theatre Anthropology (ISTA) during its fifteenth session in Albino in
   April 2016. It reflects upon the common elements underlying the
   different approaches to training in the different traditions that were
   represented by actors who are considered as `masters' in their specific
   art forms. It highlights different techniques from both the West and
   Asia that were demonstrated and discussed. It takes a look at daily life
   during ISTA, as well as the training that the participants were asked to
   undergo.},
