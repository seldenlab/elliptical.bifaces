@article{ WOS:000494305600003,
Author = {Keevallik, Leelo and Ekstrom, Anna},
Title = {How to Take the Floor as a Couple: Turn-Taking in Lindy Hop Jam Circles},
Journal = {VISUAL ANTHROPOLOGY},
Year = {2019},
Volume = {32},
Number = {5},
Pages = {423-444},
Month = {OCT 20},
Abstract = {This article analyzes the tacit norms of embodied turn-taking in a
   specific dance activity, Lindy Hop jam circles. Building on an extensive
   tradition of scrutinizing turn-taking in conversation, it shows how
   dancing couples negotiate the right to a next turn by visual means.
   Using multimodal interaction analysis, the article dissects the behavior
   of the exiting couple, the next dancing couple, and the spectators. The
   analysis shows that music is but one factor in turn-taking, and that
   maximally three publicly visible steps are necessary for a successful
   entrance: displaying `couplehood,' displaying imminent entrance, and
   occupying the exclusive central space. In a case of competition the
   dancers' speed and determination are decisive.},
