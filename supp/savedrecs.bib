@article{ WOS:000072204000009,
Author = {Adler, A},
Title = {On kings and masks. A comparative analysis of the Mundang (Chad) and
   Bushong (ex-Zaire)},
Journal = {HOMME},
Year = {1998},
Number = {145},
Pages = {169-203},
Month = {JAN-MAR},
Abstract = {The question of the relation between the king and masks in Africa is
   approached by studying two completely opposite cases. Among the Mundang,
   masks are by nature and function ``clan things{''}. Their field of
   action primarily encompasses initiatory ceremonies from which the king,
   deemed to come from no dan, is excluded. Among the Bushong on the
   contrary, the king is an eminent person who wears masks and dances.
   Through the mask representing Woot, his ancestor who founded the ruling
   dynasty, the king has the leading role in transforming young people
   through initiation. The king-mask relation thus turns out to be of great
   interest for both political and religious anthropology.},
