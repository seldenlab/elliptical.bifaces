@article{ WOS:000211319700005,
Author = {Johnson, Ginger A. and Pfister, Anne E. and Vindrola-Padros, Cecilia},
Title = {Drawings, Photos, and Performances: Using Visual Methods with Children},
Journal = {VISUAL ANTHROPOLOGY REVIEW},
Year = {2012},
Volume = {28},
Number = {2},
Pages = {164-178},
Month = {NOV},
Abstract = {Children have largely been overlooked as research participants in
   anthropological work. The anthropology of childhood has played an
   instrumental role in bringing attention to this fact, and it has
   highlighted important contributions children's viewpoints can make to
   our discipline. In this article, we present three case studies that
