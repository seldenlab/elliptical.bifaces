@article{ WOS:000073598300004,
Author = {Toffin, G},
Title = {The drum and the city: From ethnomusicology to urban anthropology
   (Nepal).},
Journal = {HOMME},
Year = {1998},
Number = {146},
Pages = {113-142},
Month = {APR-JUN},
Abstract = {In the old Newar City of Kathmandu, Nepal's capital, the dhimay drums of
   the Jyapu peasants are an organic element of urban space and a factor
   underlying elementary social bonds. Each of the Thirty-Two Jyapu
   Quarters is linked to a drumming society, which mandatorily includes all
   the territorial unit's peasants. This exclusive and permanent linkage is
   transmitted in the patriline. Moreover, these Quarters form exogamous
   units. At the center of this musicospatial arrangement, Nasahydyah, the
   Newar god of music, dance and theater, transforms each dhimay
   performance into a solemn ceremony for which the drummers must be
   prepared, initiated and religiously qualified. From the outset, musical
   practices are placed in the sacred sphere. They fit into a universe of
   religious signs from which women are excluded and where death comes into
   contact with the foundations of the social and religious order. The
   structural as well as founding role of music thus comes to light in a
   town that is changing at a fast pace into an ever more cosmopolitan
   city.},
