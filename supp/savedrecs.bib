@article{ WOS:000421978200008,
Author = {Pompa, Pierangelo},
Title = {Mei Lanfang's Eurasian scrutiny},
Journal = {TEATRO E STORIA},
Year = {2010},
Volume = {31},
Pages = {167+},
Abstract = {While reviewing foreign performances in the Popular China of the
   nineteen fifties, during the period of the realist reform of traditional
   theatre, Mei Lanfang notes resemblances and common principles between
   the recitative technique of jingju (Peking Opera) and those of other
   formalised genres such as Kabuki, classical ballet and certain Indian
   dances. Foreign theatres become a prestigious point of reference to
   defend traditional acting. The study is centred around the translation
   from the Chinese, of certain articles contained in the Mei Lanfang Wenji
   collection (1962), where the Chinese technical jargon is interpreted
   through the categories of theatre anthropology.},
