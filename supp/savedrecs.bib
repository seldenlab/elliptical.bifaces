@article{ WOS:000222295300005,
Author = {Gabbay, U},
Title = {Dance in textual sources from ancient Mesopotamia},
Journal = {NEAR EASTERN ARCHAEOLOGY},
Year = {2003},
Volume = {66},
Number = {3},
Pages = {103-105},
Month = {SEP},
Abstract = {The textual sources from Mesopotamia portray dance, both as a private
   act of spontaneous joy and play and as a cultic act performed in
   religious rituals. Its connection with sexuality can be seen in a few
   passages where the dance of girls and women is mentioned in a sexual
   context and in passages describing the cult dances performed by the
   cross-dressing personnel of the goddess Ishtar.
   The joy and vitality associated with dancing is illustrated in a letter
   to King Assurbanipal (seventh century BCE) in which the Assyrian scholar
   Adad-shumu-usur describes the reign of his king as a golden era.},
