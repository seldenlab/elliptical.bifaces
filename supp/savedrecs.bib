@article{ WOS:000228615100005,
Author = {Rothfield, P},
Title = {Differentiating phenomenology and dance},
Journal = {TOPOI-AN INTERNATIONAL REVIEW OF PHILOSOPHY},
Year = {2005},
Volume = {24},
Number = {1},
Pages = {43-53},
Abstract = {This paper critically reviews phenomenological philosophy of the body in
   light of postmodern and postcolonial critiques of universalism. It aims
   to recast the notion of the lived body in plural rather than singular
   terms. It does so within the context of phenomenology and dance, using
   cultural anthropology to highlight the sense in which bodies are
   culturally and corporeally specific. The notion of corporeal specificity
   is applied to the perception of dance, paying particular attention to
   questions or power and hegemony. This is not to reject phenomenology,
   but to differentiate it according to its social, historical and
   kinaesthetic milieux.},
