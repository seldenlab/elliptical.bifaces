@article{ WOS:000440643000009,
Author = {Rodrigues, Shirley},
Title = {DANCE AND KNOWLEDGE: THE HUMAN ABILITY TO DANCE IN CONNECTION WITH THE
   CONCEPT OF KNOWLEDGE OF DANCE},
Journal = {PERCEVEJO ONLINE},
Year = {2014},
Volume = {6},
Number = {1},
Pages = {129-151},
Month = {JAN-JUN},
Abstract = {Dance is presents in different social spaces, and its knowledge and
   development is multifarious, as in popular dance, classic dance, in
   therapy and pedagogy, as well as in the fields of research of
   neuroscience, science of dance, anthropology and of philosophy. Dance
   features a rich potential in terms of human capacity and of the
   production of knowledge. In this sense, this article proposes to
   introduce the concept of bodily knowing, which is the departure point
   from which I perform a theoretical revision of the frameworks of dance
   studies. In order to perform this analysis, I investigate the human
   ability to dance in connection with the concept of dance knowledge.},
