@article{ WOS:000448736200007,
Author = {Vellet, Joelle},
Title = {The matrix transmission of modern dance},
Journal = {STAPS-SCIENCES ET TECHNIQUES DES ACTIVITES PHYSIQUES ET SPORTIVES},
Year = {2006},
Volume = {27},
Number = {72},
Pages = {79-91},
Month = {SPR},
Abstract = {Our research is about the transmission activity of a famous
   choreographer, Odile Duboc, when she is teaching her own dance to very
   highly qualiefed teachers. What is teaching is a short choreographic
   variation on modern dance. We describe how Odile Duboc is doing to
   transmit the dance. We are trying to understand the specifity of this
   transmission and what is given to the teachers to dance that they can't
   see and understand when they are just looking for the dance video (as
   first they had only the video to learn the dance). During the
   observation and analysis of this transmission process we underline that
   the choreographer uses discourses in different intentions. And it's to
   give access to the origin of the movement and the dance genesis, which
   is necessary for teachers to understand the dance. We suggest four
   functions of the discourses in situation and like this we can propose
   the notion of what we have called the matrix transmission.},
