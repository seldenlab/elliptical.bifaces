Language = {English},
Affiliation = {Rappoport, D (Corresponding Author), CNRS, CASE, 54 Blvd Raspail, F-75006 Paris, France.
   Rappoport, Dana, Ctr Asie Sud Est, Paris, France.},
DOI = {10.1080/14442213.2017.1372515},
ISSN = {1444-2213},
EISSN = {1740-9314},
Keywords = {Songs; Dance; Stones; Spirits; Eastern Indonesia; Lamaholot; Flores;
