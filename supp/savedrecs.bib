@article{ WOS:000453959000043,
Author = {Shcherbakov, V. V.},
Title = {Cultural aspects of categorization ``dance{''}},
Journal = {NATIONAL ACADEMY OF MANAGERIAL STAFF OF CULTURE AND ARTS HERALD},
Year = {2013},
Number = {4},
Pages = {219-225},
Abstract = {In this article an attempt to expose the revealing of a
   culturalurological aspect of a category ``dance{''} has been undertaken.
   Dance - a huge reservoir of human culture, which absorbs and transmits
   its own methods and means all inherited a complex social ways of life
   and beliefs, which is the essence of our being. Dance is not just has
   its place in the socio-cultural reality, but also reflects the whole
