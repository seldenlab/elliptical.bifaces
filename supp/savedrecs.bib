@article{ WOS:000217818800005,
Author = {Ferdon, Jr., Edwin N.},
Title = {THE HOHOKAM ``BALL COURT{''} AN ALTERNATE VIEW OF ITS FUNCTION},
Journal = {KIVA-JOURNAL OF SOUTHWESTERN ANTHROPOLOGY AND HISTORY},
Year = {2009},
Volume = {75},
Number = {2},
Pages = {165-178},
Abstract = {A comparative study of Hohokam and Mesoamerican ball courts indicates a
   lack of shared fundamental features. It is suggested, therefore, that
   Hohokam courts may have served some other function. General
   similarities, including basic court orientations, with the Papago Vikita
   dance court assemblages at Santa Rosa and Quitovaca may reflect a
   similar use of Hohokam courts in prehistoric times.},
