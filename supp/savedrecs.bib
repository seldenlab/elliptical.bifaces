@article{ WOS:000707736900001,
Author = {Johnson, Sarah Jean and de la Piedra, Maria Teresa and Sanmiguel-Lopez,
   Alejandra and Perez-Piza, Maria},
Title = {Mexican-heritage Children's Learning of Ballet Folklorico: Herencia,
   Familia, y Orgullo},
Journal = {ANTHROPOLOGY \& EDUCATION QUARTERLY},
Year = {2022},
Volume = {53},
Number = {2},
Pages = {130-148},
Month = {APR},
Abstract = {This case study examines Mexican-heritage children's learning to dance
   ballet folklorico. Drawing from an interpretive, ethnographic approach,
   we argue the practices associated with the dance exist within
   encompassing domains of meaning that are individually enhancing while
   also prosocial, encouraging membership to the folklorico group and
   broader cultural community. These domains are presented as
   ``herencia{''} (heritage), ``familia{''} (family), and ``orgullo{''}
   (pride) along with a discussion of how they relate to access and equity
   in arts learning.},
