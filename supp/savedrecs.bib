@article{ WOS:000421298300006,
Author = {Diaz Rivas, Manuel},
Title = {NOSTALGIA AS METHOD IN ANTHROPOLOGY},
Journal = {BOLETIN ANTROPOLOGICO},
Year = {2014},
Volume = {2},
Number = {88},
Pages = {165-171},
Month = {JUL-DEC},
Abstract = {In the fram of the Doctorate in Anthropology at the University of the
   Andes in Merida, Venezuela, research has been concluded on a dance form
   called joropo which originates in the central plains area. Dance forms
   have been viewed as ritualistic and as mythical, using the terminology
   of the Venezuelan anthropologist Rafael Lopez Sanz. This perspective
   encourages and sustains the positing of nostalgia as a feeling which we
   have lived through, and the telling of it by those who have known what
   is it as a personal experience. The present paper is a reflection on
   this experience which we have designated as the ``methodology of
   nostalgia{''}.},
